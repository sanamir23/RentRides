<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: lightgreen;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.renter.ReturnVehicleController">
    <Label layoutX="43.0" layoutY="148.0" text="Enter Vehicle Registration Plate:">
      <font>
         <Font name="System Bold" size="11.0" />
      </font></Label>
    <TextField fx:id="registrationPlateField" layoutX="225.0" layoutY="143.0" prefHeight="26.0" prefWidth="238.0" promptText="Registration Plate" />
    
    <Label layoutX="43.0" layoutY="191.0" prefHeight="18.0" prefWidth="166.0" text="Enter Your Username:">
      <font>
         <Font name="System Bold" size="12.0" />
      </font></Label>
    <TextField fx:id="usernameField" layoutX="225.0" layoutY="188.0" prefHeight="26.0" prefWidth="238.0" promptText="Username" />
    
    <Label layoutX="43.0" layoutY="241.0" prefHeight="19.0" prefWidth="166.0" text="Select Return Date:">
      <font>
         <Font name="System Bold" size="13.0" />
      </font></Label>
    <DatePicker fx:id="returnDatePicker" layoutX="225.0" layoutY="238.0" prefHeight="26.0" prefWidth="238.0" />
    
    <Button layoutX="225.0" layoutY="302.0" onAction="#handleReturnVehicle" prefWidth="150" text="Return Vehicle" />
    
    <Label fx:id="statusLabel" layoutX="20" layoutY="180" prefWidth="400" text="" />
   <Label layoutX="217.0" layoutY="56.0" text="Return Vehicle">
      <font>
         <Font name="Times New Roman Bold" size="24.0" />
      </font>
   </Label>
   <Button fx:id="backButton" layoutX="43.0" layoutY="351.0" mnemonicParsing="false" onAction="#handleBackAction" text="Back" />
</AnchorPane>
