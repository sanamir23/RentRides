<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400" prefWidth="600" style="-fx-background-color: lightgreen;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.renter.ContactSupportController">
    <Label layoutX="19.0" layoutY="88.0" prefHeight="18.0" prefWidth="64.0" text="Username" />
    <TextField fx:id="renterIDField" layoutX="120.0" layoutY="84.0" prefHeight="26.0" prefWidth="380.0" promptText="Enter Username" />

    <Label layoutX="17.0" layoutY="138.0" text="Issue Type" />
    <ComboBox fx:id="issueTypeComboBox" layoutX="120.0" layoutY="134.0" prefHeight="26.0" prefWidth="380.0" />
   

    <Label layoutX="14.0" layoutY="182.0" text="Issue Description">
      <font>
         <Font size="13.0" />
      </font></Label>
    <TextArea fx:id="issueDescriptionArea" layoutX="120.0" layoutY="182.0" prefHeight="100" prefWidth="380.0" promptText="Describe the issue..." wrapText="true" />

    <Button fx:id="submitButton" layoutX="200.0" layoutY="306.0" onAction="#handleSubmit" prefWidth="200" text="Submit Issue" />
   <Label layoutX="160.0" layoutY="33.0" text="Customer Support Form ">
      <font>
         <Font name="System Bold" size="24.0" />
      </font>
   </Label>
   <Button fx:id="backButton" layoutX="25.0" layoutY="355.0" mnemonicParsing="false" onAction="#handleBackAction" text="Back" />
</AnchorPane>
