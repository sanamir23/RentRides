<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="10" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: lightgreen;" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="application.renter.VehicleDetailsController">
    <TextArea fx:id="detailsTextArea" editable="false" prefHeight="177.0" prefWidth="478.0" wrapText="true" />
    <Button fx:id="saveButton" text="Save for Later" />
    <Button fx:id="rentButton" text="Rent Vehicle" />
    <Button fx:id="backButton" onAction="#initialize" text="Back to Browsing" />
</VBox>
